class Program 
{
	int a[100];
	int binarysearch(int l, int r, int x)
	{
		if(l < r)
		{
			int mid;
			mid = l + (r-l)/2;
			if(a[mid]==x)
				return mid;
			if(a[mid] > x)
				return binarysearch(l,mid-1,x);
			else
				return binarysearch(mid+1,r,x);
		}
		return -1;
	}
	void main()
	{
		int n,x,ind;
		callout("scanf", "%d", &n);
		for i=0,n-1
			callout("scanf", "%d", &a[i]);
		callout("scanf", "%d", &x);
		ind = binarysearch(0, n-1, x);
		if( ind==-1)
			callout("printf", "Number doesnot exist\n");
		else
			callout("printf", "Required number is at %d position in\n",ind);
	}
}